<!DOCTYPE html>
<html>
  <head>
    <title>Myapp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
        <div class="col-xs-3">
          <div class="circle">
          <br />
          Mental
          <br />
          Physical
          <br />
          Emotional
        </div>
      </div>
      
      <div class="col-xs-4 slogan">
        Knowledge is meant to use not to know.
      </div>
      
      <div class="col-xs-5 links">
        <%= link_to 'Contribute', '#' %>
        <%= link_to 'About', '#' %>
      </div>
    </header>
    <hr />
    <br class"clear" />
    
    <%= yield %>
    
  </body>
</html>
